{
    "name": "larasun/larasun",
    "type": "project",
    "description": "",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "larasun/framework": "^0.0.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "Illuminate\\": "packages/larasun/framework/src/Illuminate/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Illuminate\\Tests\\": "tests/"
      }
    },
    "repositories": [
      {
        "type": "path",
        "url": "../library",
        "options": {
          "symlink": false
        }
      }
    ]
}
